{
    "name": "cedricchavaudra/pdftohtml-php",
    "description": "PDF to HTML converter with PHP using Poppler-utils",
    "license": "MIT",
    "authors": [
        {
            "name": "Mochamad Gufron",
            "email": "mgufronefendi@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "paquettg/php-html-parser": "~2",
        "illuminate/config": "~5"
    },
    "require-dev":{
        "phpunit/phpunit":"~4",
        "satooshi/php-coveralls": "^1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cedricchavaudra/pdf-to-html.git"
        }
    ],
    "autoload": {
        "psr-0": {
            "Gufy": "src"
        },
        "psr-4": {
            "Gufy\\PdfToHtml\\": "src/"
        }
    }
}
